if (intLength != CORRECT_LENGTH) {
			System.out.println(testSsNumber + " Not a valid Social Security number. Try again.");
			System.exit(0);
		}
		
		for (int i =0; i <=2; i++) {
			if (Character.isDigit(testSsNumber.charAt(i))){
				continue;
			}
				else {
					System.out.println(testSsNumber + " is NOT a valid Social Security number.");
					System.exit(0);
				}
			}
		
		for (int i= 3; ; ) {
			if (testSsNumber.charAt(i) == '-') {
				continue;
			}
				else {
					System.out.println(testSsNumber + " is NOT a valid Social Security number.");	
					System.exit(0);	
				}
			}
	
		
		for (int i = 4 ; i <= 5; i++) {
			if (Character.isDigit(testSsNumber.charAt(i))) {
			continue;
			}
			else { 
				System.out.println(testSsNumber + " is NOT a valid Social Security number.");	
				System.exit(0);
			}
		}
		
		for (int i = 6 ; i <= 10; i++) {
			if (Character.isDigit(testSsNumber.charAt(i))) {
				continue;
			}
			else {
				System.out.println(testSsNumber + " is NOT a valid Social Security number.");
			}
		}
		
		
